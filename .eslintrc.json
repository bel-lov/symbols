{
    "env": {
        "jest": true,
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-restricted-syntax": [
            "error",
            "LabeledStatement",
            "WithStatement"
        ],
        "no-console": 0,
        "no-alert": 0,
        // "no-unused-vars": 0,
        "no-plusplus": "off"
    }
}